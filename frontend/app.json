{
  "expo": {
    "name": "Proveit",
    "slug": "ProveIt",
    "version": "0.8",
    "orientation": "portrait",
    "icon": "./src/assets/proveitIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/proveitIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff7152"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/proveitIcon.png",
        "backgroundColor": "#ff7152"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.renandevs.ProveIt"
    },
    "web": {
      "favicon": "./src/assets/proveitIcon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e5f3a49c-7f63-455d-8008-43b4f0a4f6a1"
      }
    }
  }
}
